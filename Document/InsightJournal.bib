pmid = {24023588},
title = {{Scale-specific multifractal medical image analysis.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3760300\&tool=pmcentrez\&rendertype=abstract},
volume = {2013},
