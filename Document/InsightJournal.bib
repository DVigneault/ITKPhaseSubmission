journal = {International journal of cardiology},
keywords = {African Continental Ancestry Group,African Continental Ancestry Group: statistics \& n,Developing Countries,Heart Diseases,Heart Diseases: diagnosis,Heart Diseases: epidemiology,Heart Diseases: ethnology,Heart Diseases: etiology,Humans,Incidence,Prevalence,Registries,Research Design,Risk Factors,Socioeconomic Factors,South Africa,South Africa: epidemiology,Urban Population,Urban Population: statistics \& numerical data},
