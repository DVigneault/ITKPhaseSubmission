isbn = {0780387406},
issn = {1557-170X},
journal = {Conference proceedings : ... Annual International Conference of the IEEE Engineering in Medicine and Biology Society. IEEE Engineering in Medicine and Biology Society. Annual Conference},
month = jan,
pages = {4298--301},
pmid = {17281185},
title = {{Improved Harmonic Phase (HARP) Method for Motion Tracking a Tagged Cardiac MR images.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17281185},
volume = {4},
year = {2005}
}
