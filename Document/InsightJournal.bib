keywords = {Adult,Female,Heart Ventricles,Heart Ventricles: metabolism,Humans,Image Enhancement,Magnetic Resonance Angiography,Magnetic Resonance Angiography: methods,Magnetic Resonance Angiography: standards,Male,Myocardial Contraction,Young Adult},
