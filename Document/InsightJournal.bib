issn = {1522-2594},
journal = {Magnetic resonance in medicine : official journal of the Society of Magnetic Resonance in Medicine / Society of Magnetic Resonance in Medicine},
keywords = {fractional shortening,myocardial strain,ssfp},
month = mar,
