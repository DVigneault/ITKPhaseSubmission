journal = {Journal of the American College of Cardiology},
keywords = {Adipose Tissue,Adipose Tissue: pathology,Adolescent,Adult,Arrhythmogenic Right Ventricular Dysplasia,Arrhythmogenic Right Ventricular Dysplasia: diagno,Arrhythmogenic Right Ventricular Dysplasia: physio,Female,Humans,Magnetic Resonance Imaging,Male,Middle Aged,Myocardium,Myocardium: pathology,Observer Variation,Sensitivity and Specificity,Ventricular Function, Right},
month = dec,
