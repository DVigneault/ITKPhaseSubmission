title = {{Quantitative susceptibility mapping of human brain reflects spatial variation in tissue composition.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3062654\&tool=pmcentrez\&rendertype=abstract},
volume = {55},
year = {2011}
}
