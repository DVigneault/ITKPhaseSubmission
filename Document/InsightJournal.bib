issn = {1532-429X},
journal = {Journal of cardiovascular magnetic resonance : official journal of the Society for Cardiovascular Magnetic Resonance},
keywords = {Adolescent,Adult,African Continental Ancestry Group,African Continental Ancestry Group: statistics \& n,Aged,Aged, 80 and over,Chi-Square Distribution,Echocardiography,European Continental Ancestry Group,European Continental Ancestry Group: statistics \&,Fractals,Humans,Image Interpretation, Computer-Assisted,Image Interpretation, Computer-Assisted: methods,Isolated Noncompaction of the Ventricular Myocardi,Magnetic Resonance Imaging, Cine,Magnetic Resonance Imaging, Cine: methods,Middle Aged,Predictive Value of Tests,ROC Curve,Reproducibility of Results,Sensitivity and Specificity,Ventricular Dysfunction, Left,Ventricular Dysfunction, Left: ethnology,Ventricular Dysfunction, Left: pathology},
