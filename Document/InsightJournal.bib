pages = {251--9},
pmid = {16855265},
title = {{Trends in prevalence and outcome of heart failure with preserved ejection fraction.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/16855265},
volume = {355},
year = {2006}
}
