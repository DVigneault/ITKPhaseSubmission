issn = {1053-1807},
journal = {Journal of magnetic resonance imaging : JMRI},
keywords = {Algorithms,Humans,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Phlebography,Veins,Veins: anatomy \& histology},
month = mar,
number = {3},
pages = {649--52},
pmid = {18183579},
title = {{A modified Fourier-based phase unwrapping algorithm with an application to MRI venography.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2575666\&tool=pmcentrez\&rendertype=abstract},
