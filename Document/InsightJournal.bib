issn = {1558-3597},
journal = {Journal of the American College of Cardiology},
keywords = {Adolescent,Adult,Aged,Aged, 80 and over,Cardiomyopathy, Hypertrophic,Cardiomyopathy, Hypertrophic: diagnosis,Cardiomyopathy, Hypertrophic: genetics,Cardiomyopathy, Hypertrophic: radionuclide imaging,Cardiomyopathy, Hypertrophic: ultrasonography,Child,Echocardiography,Female,Fibrosis,Gadolinium,Gadolinium: diagnostic use,Humans,Hypertrophy, Left Ventricular,Hypertrophy, Left Ventricular: diagnosis,Hypertrophy, Left Ventricular: genetics,Hypertrophy, Left Ventricular: radionuclide imagin,Hypertrophy, Left Ventricular: ultrasonography,Magnetic Resonance Imaging,Male,Middle Aged,Phenotype,Prospective Studies,Time Factors,Ventricular Outflow Obstruction,Ventricular Outflow Obstruction: diagnosis,Ventricular Outflow Obstruction: genetics,Young Adult},
