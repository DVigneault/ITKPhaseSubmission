issn = {1558-3597},
journal = {Journal of the American College of Cardiology},
keywords = {Aged,Anti-Arrhythmia Agents,Anti-Arrhythmia Agents: therapeutic use,Cause of Death,Chi-Square Distribution,Confidence Intervals,Coronary Artery Disease,Coronary Artery Disease: diagnosis,Coronary Artery Disease: mortality,Coronary Artery Disease: therapy,Death, Sudden, Cardiac,Death, Sudden, Cardiac: epidemiology,Defibrillators, Implantable,Female,Heart Arrest,Heart Arrest: mortality,Humans,Male,Middle Aged,Predictive Value of Tests,Probability,Proportional Hazards Models,Risk Assessment,Sensitivity and Specificity,Stroke Volume,Tachycardia, Ventricular,Tachycardia, Ventricular: diagnosis,Tachycardia, Ventricular: mortality,Tachycardia, Ventricular: therapy},
